        @*Yaklaşan İzinler*@
        <div class="col-xl-3">
            <!--begin::List Widget 2-->
            <div class="shadow p-3 mb-5 bg-white rounded card card-custom bg-light-white card-stretch gutter-b">
                <!--begin::Header-->
                <div class="card-header border-0">
                    <h3 class="card-title font-weight-bolder text-success">
                        <a class="cursor-pointer" data-toggle="modal" data-target="#staffUpcomingLeaveApprove">Yaklaşan İzinler</a>
                    </h3>
                </div>
                <!--end::Header-->
                <!--begin::Body-->
                <div class="card-body pt-2">
                    @if (Model.UpcomingLeaves.Count > 0)
                    {
                        var counter = Model.UpcomingLeaves.Count >= 4 ? 4 : Model.UpcomingLeaves.Count;
                        @for (int i = 0; i < counter; i++)
                        {
                            <div class="d-flex align-items-center flex-wrap mb-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-50  mr-5 symbol-light-@colors[rand.Next(5)]" flex-shrink-0">
                                    <div class="symbol-label font-size-h1"> @Model.UpcomingLeaves[i].NameSurname.Substring(0, 1)</div>
                                </div>
                                <!--end::Symbol-->
                                <!--begin::Text-->
                                <div class="d-flex flex-column flex-grow-1 mr-2">
                                    <a asp-controller="StaffGeneral" asp-action="Index" asp-route-id=" @Model.UpcomingLeaves[i].StaffId" class="font-weight-bold text-dark-75 text-hover-primary font-size-lg mb-1">@Model.UpcomingLeaves[i].NameSurname</a>
                                    <span class="text-muted font-weight-bold">@Model.UpcomingLeaves[i].LeaveStartDate.ToString("d")</span>
                                </div>
                            </div>
                        }
                        if (Model.UpcomingLeaves.Count > 4)
                        {
                            <div class="text-muted text-center font-weight-bold  mt-1"><a  class="font-size-xs text-primary cursor-pointer"  data-toggle="modal" data-target="#staffUpcomingLeaveApprove" >Tümünü Gör</a></div>
                        }
                    }
                    else
                    {

                        <div class="d-flex align-items-center flex-wrap mb-5">
                            <!--begin::Symbol-->
                            <div class="symbol symbol-50 symbol-white pl-0">
                                <span>
                                    <i class="fas fa-exclamation-circle fa-2x text-warning"></i>
                                </span>
                            </div>
                            <!--end::Symbol-->
                            <!--begin::Text-->
                            <div class="d-flex flex-column flex-grow-1 ml-2">
                                <span> Yaklaşan izin bulunmamaktadır</span>
                            </div>
                            <!--end::Text-->
                        </div>
                    }

                </div>
                <!--end::Body-->
            </div>
            <!--end::List Widget 2-->
        </div>

        <div class="modal fade" id="staffUpcomingLeaveApprove" tabindex="-1" role="dialog" aria-labelledby="staffCurrentlyOnLeaveApprove" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered modal-md" role="document">
                <div class="modal-content">
                    <div class="modal-header ">
                        <div class="card-title align-items-start flex-column">
                            <h3 class="card-label font-weight-bolder ">
                                <i class="fas fa-umbrella-beach mr-2 "></i>Yaklaşan İzinler
                            </h3>
                        </div>
                        <button type="button" class="link-2 text-white bg-danger " data-dismiss="modal" aria-label="Close">
                        </button>
                    </div>
                    <div class="modal-body" style="max-height: 300px; overflow-x: hidden; overflow-y: scroll;">
                        @if (Model.UpcomingLeaves.Count > 0)
                        {
                            @foreach (var item in Model.UpcomingLeaves)
                            {
                                <div class="d-flex align-items-center flex-wrap mb-5">
                                    <!--begin::Symbol-->
                                    <div class="symbol symbol-50  mr-5 symbol-light-@colors[rand.Next(5)]" flex-shrink-0">
                                        <div class="symbol-label font-size-h1"> @item.NameSurname.Substring(0, 1)</div>
                                    </div>
                                    <!--end::Symbol-->
                                    <!--begin::Text-->
                                    <div class="d-flex flex-column flex-grow-1 mr-2">
                                        <a asp-controller="StaffGeneral" asp-action="Index" asp-route-id="@item.StaffId" class="font-weight-bold text-dark-75 text-hover-primary font-size-lg mb-1">@item.NameSurname</a>
                                        <span class="text-muted font-weight-bold">@item.LeaveStartDate.ToString("d") - @item.LeaveEndDate.ToString("d")</span>
                                    </div>
                                    <span class="label label-xl label-light label-inline my-lg-0 my-2 text-dark-50 font-weight-bolder"> @item.LeaveType</span>
                                </div>
                            }
                        }
                        else
                        {

                            <div class="d-flex align-items-center flex-wrap mb-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-50 symbol-white pl-0">
                                    <span>
                                        <i class="fas fa-exclamation-circle fa-2x text-warning"></i>
                                    </span>
                                </div>
                                <!--end::Symbol-->
                                <!--begin::Text-->
                                <div class="d-flex flex-column flex-grow-1 ml-2">
                                    <span> Yaklaşan izin bulunmamaktadır</span>
                                </div>
                                <!--end::Text-->
                            </div>
                        }

                    </div>

                </div>
            </div>
        </div>

        @*Doğum günleri*@
        <div class="col-xl-3">
            <!--begin::List Widget 2-->
            <div class="shadow p-3 mb-5 bg-white rounded card card-custom bg-light-white card-stretch gutter-b">
                <!--begin::Header-->
