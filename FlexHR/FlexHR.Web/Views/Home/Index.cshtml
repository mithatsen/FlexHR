                                    <span class="text-muted font-weight-bold">@Model.CurrentlyOnLeave[i].LeaveEndDate.ToString("d")</span>
                                </div>
                                <!--end::Text-->
                                <span class="label label-xl label-light label-inline my-lg-0 my-2 text-dark-50 font-weight-bolder"> <a onclick="getCurrentlyOnStaffLeaveApproveModal(@Model.CurrentlyOnLeave[i].StaffLeaveId)" class="cursor-pointer">Detay</a></span>
                            </div>

                        }
                        if (Model.CurrentlyOnLeave.Count > 4)
                        {
                            <div class="text-muted text-center font-weight-bold  mt-1"><a class="font-size-xs" asp-controller="Leave" asp-action="Index">Tümünü Gör</a></div>
