@model ListDashboardViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{ Random rand = new Random();
    List<string> colors = new List<string> { "success", "danger", "info", "primary", "warning" };
}
<div class="subheader py-2 py-lg-4 subheader-solid" id="kt_subheader">
    <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
        <!--begin::Info-->
        <div class="d-flex align-items-center flex-wrap mr-2">
            <!--begin::Page Title-->
            <!--end::Page Title-->
            <!--begin::Actions-->
            <div class="subheader-separator subheader-separator-ver mt-2 mb-2 mr-4 bg-gray-200"></div>
            <h5 class="text-dark font-weight-bold mt-2 mb-2 mr-5">Hoşgeldin @ViewBag.StaffName</h5>
            <!--end::Actions-->
        </div>
        <!--end::Info-->


    </div>
</div>
<div class="d-flex flex-column-fluid">
    <!--begin::Dashboard-->
    <!--begin::Row-->
    <div class="row">
        @*Harcama Talepleri*@
        <div class="col-xl-3">
            <!--begin::List Widget 2-->
            <div class="shadow p-3 mb-5 bg-white rounded card card-custom bg-light-white card-stretch gutter-b">
                <!--begin::Header-->
                <div class="card-header border-0">
                    <h3 class="card-title font-weight-bolder text-success">
                        <a class="text-success" asp-controller="Payment" asp-action="Index">Ödeme Talepleri</a>
                    </h3>
                    <div class="card-toolbar"> <span class="badge badge-success">@Model.StaffPayments.Count</span></div>
                </div>

                <!--end::Header-->
                <!--begin::Body-->
                <div class="card-body pt-2">

                    @if (Model.StaffPayments.Count > 0)
                    {
                        var counter = Model.StaffPayments.Count >= 4 ? 4 : Model.StaffPayments.Count;
                        @for (int i = 0; i < counter; i++)
                        {
                            <div class="d-flex align-items-center flex-wrap mb-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-50  mr-5 symbol-light-@colors[rand.Next(5)]" flex-shrink-0">
                                    <div class="symbol-label font-size-h1"> @Model.StaffPayments[i].NameSurname.Substring(0, 1)</div>
                                </div>
                                <!--end::Symbol-->
                                <!--begin::Text-->
                                <div class="d-flex flex-column flex-grow-1 mr-2">
                                    <a asp-controller="StaffGeneral" asp-action="Index" asp-route-id="@Model.StaffPayments[i].StaffId" class="font-weight-bold text-dark-75 text-hover-primary font-size-lg mb-1">@Model.StaffPayments[i].NameSurname</a>
                                    <span class="text-muted font-weight-bold">@Model.StaffPayments[i].CreationDate.ToString("d")</span>
                                </div>
                                <!--end::Text-->
                                <a onclick=getStaffPaymentApproveModal(@Model.StaffPayments[i].StaffPaymentId) class="cursor-pointer"><span class="label label-xl label-light label-inline my-lg-0 my-2 text-dark-50 font-weight-bolder">Detay</span></a>
                            </div>
                        }
                        if (Model.StaffPayments.Count > 4)
                        {
                            <div class="text-muted text-center font-weight-bold  mt-1"><a class="font-size-xs" asp-controller="Payment" asp-action="Index">Tümünü Gör</a></div>
                        }
                    }
                    else
                    {

                        <div class="d-flex align-items-center flex-wrap mb-5">
                            <!--begin::Symbol-->
                            <div class="symbol symbol-50 symbol-white pl-0">
                                <span>
                                    <i class="fas fa-exclamation-circle fa-2x text-warning"></i>
                                </span>
                            </div>
                            <!--end::Symbol-->
                            <!--begin::Text-->
                            <div class="d-flex flex-column flex-grow-1 ml-2">
                                <span> Ödeme talebi bulunmamaktadır</span>
                            </div>
                            <!--end::Text-->
                        </div>
                    }

                </div>
                <!--end::Body-->
            </div>
            <!--end::List Widget 2-->
        </div>
        @*Mesai Talepleri*@
        <div class="col-xl-3">
            <!--begin::List Widget 2-->
            <div class="shadow p-3 mb-5 bg-white rounded card card-custom bg-light-white card-stretch gutter-b">
                <!--begin::Header-->
                <div class="card-header border-0">
                    <h3 class="card-title font-weight-bolder text-success">
                        <a class="text-success" asp-controller="Shift" asp-action="Index">Mesai Talepleri</a>
                    </h3>
                    <div class="card-toolbar"> <span class="badge badge-success">@Model.StaffShifts.Count</span></div>
                </div>

                <!--end::Header-->
                <!--begin::Body-->
                <div class="card-body pt-2">
                    <!--begin::Item-->

                    @if (Model.StaffShifts.Count > 0)
                    {
                        var counter = Model.StaffShifts.Count >= 4 ? 4 : Model.StaffShifts.Count;
                        @for (int i = 0; i < counter; i++)
                        {
                            <div class="d-flex align-items-center flex-wrap mb-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-50  mr-5 symbol-light-@colors[rand.Next(5)]" flex-shrink-0">
                                    <div class="symbol-label font-size-h1"> @Model.StaffShifts[i].NameSurname.Substring(0, 1)</div>
                                </div>
                                <!--end::Symbol-->
                                <!--begin::Text-->
                                <div class="d-flex flex-column flex-grow-1 mr-2">
                                    <a asp-controller="StaffGeneral" asp-action="Index" asp-route-id="@Model.StaffShifts[i].StaffId" class="font-weight-bold text-dark-75 text-hover-primary font-size-lg mb-1">@Model.StaffShifts[i].NameSurname</a>
                                    <span class="text-muted font-weight-bold">@Model.StaffShifts[i].StartDate.ToString("d")</span>
                                </div>
                                <!--end::Text-->
                                <a onclick=getStaffShiftApproveModal(@Model.StaffShifts[i].StaffShiftId) class="cursor-pointer"><span class="label label-xl label-light label-inline my-lg-0 my-2 text-dark-50 font-weight-bolder">Detay</span></a>
                            </div>
                        }
                        if (Model.StaffShifts.Count > 4)
                        {
                            <div class="text-muted text-center font-weight-bold  mt-1"><a class="font-size-xs" asp-controller="Shift" asp-action="Index">Tümünü Gör</a></div>
                        }
                    }
                    else
                    {

                        <div class="d-flex align-items-center flex-wrap mb-5">
                            <!--begin::Symbol-->
                            <div class="symbol symbol-50 symbol-white pl-0">
                                <span>
                                    <i class="fas fa-exclamation-circle fa-2x text-warning"></i>
                                </span>
                            </div>
                            <!--end::Symbol-->
                            <!--begin::Text-->
                            <div class="d-flex flex-column flex-grow-1 ml-2">
                                <span> Mesai talebi bulunmamaktadır</span>
                            </div>
                            <!--end::Text-->
                        </div>
                    }


                </div>
                <!--end::Body-->
            </div>
            <!--end::List Widget 2-->
        </div>

